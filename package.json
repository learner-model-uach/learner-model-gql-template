{
  "name": "learner-model-gql-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "concurrently -m 1 -r \"graphql-codegen --config codegen.yaml\" \"next build\"",
    "dev": "concurrently -r next pnpm:gql:watch pnpm:open:browser",
    "gql": "graphql-codegen --config codegen.yaml",
    "gql:watch": "graphql-codegen --config codegen.yaml --watch",
    "open:browser": "wait-on -s 1 http://localhost:3000 && open-cli http://localhost:3000",
    "pretty": "prettier --write \"**/*.{ts,tsx,json}\"",
    "start": "next start"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.12.1",
    "@chakra-ui/react": "^1.8.9",
    "@chakra-ui/system": "^1.12.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/core": "^4.0.2",
    "@graphql-typed-document-node/core": "^3.2.0",
    "framer-motion": "^6.5.1",
    "graphql": "^16.10.0",
    "next": "^12.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^5.4.0",
    "react-query": "^3.39.3",
    "rq-gql": "^0.4.3",
    "serialize-error": "^12.0.0",
    "valtio": "1.2.9"
  },
  "devDependencies": {
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@types/node": "^22.10.7",
    "@types/react": "^17.0.83",
    "@types/react-dom": "^17.0.26",
    "concurrently": "^9.1.2",
    "eslint": "9.18.0",
    "eslint-config-next": "12.3.4",
    "open-cli": "^8.0.0",
    "prettier": "^3.4.2",
    "typescript": "^4.9.5",
    "wait-on": "^8.0.2"
  },
  "engines": {
    "pnpm": ">=8.7.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core"
      ],
      "allowedVersions": {
        "graphql": "*",
        "framer-motion": "*"
      }
    }
  }
}
